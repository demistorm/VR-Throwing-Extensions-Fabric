{
	"schemaVersion": 1,
	"id": "vr-throwing-extensions-fabric",
	"version": "${version}",
	"name": "VR Throwing Extensions",
	"description": "Allows VR players to physically throw weapons and items!",
	"authors": [
		"Halfstorm"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/vr-throwing-extensions/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"win.demistorm.VRThrowingExtensions"
		],
		"client": [
			"win.demistorm.VRThrowingExtensionsClient"
		]
	},
	"mixins": [
		"vr-throwing-extensions.mixins.json",
		{
			"config": "vr-throwing-extensions.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.21.5",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}